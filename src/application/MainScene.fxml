<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.collections.FXCollections?>

<VBox prefHeight="236.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.FamilyBudgetController">
   <children>
      <HBox prefHeight="44.0" prefWidth="601.0">
         <children>
            <Label text="# of Family Members: ">
               <opaqueInsets>
                  <Insets left="10.0" />
               </opaqueInsets>
               <HBox.margin>
                  <Insets bottom="5.0" left="10.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="familyAmountTextbox">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </TextField>
            <Label text="Province: ">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Label>
            <ChoiceBox fx:id="provinceChoiceBox" prefWidth="150.0" value="Alberta">
            	<items>
            		<FXCollections fx:factory="observableArrayList">
            			<String fx:value="Alberta" />
            			<String fx:value="British Columbia" />
            			<String fx:value="Saskatchewan" />
            		</FXCollections>
            	</items>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </ChoiceBox>
            <Button mnemonicParsing="false" onAction="#getFamilyMembers" text="Done">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <ScrollPane prefHeight="154.0" prefWidth="601.0">
         <content>
            <VBox fx:id="scrollVBox" prefHeight="200.0" prefWidth="794.0" />
         </content>
      </ScrollPane>
      <HBox prefHeight="25.0" prefWidth="601.0">
         <children>
            <Button mnemonicParsing="false" onAction="#calculateTaxes" text="Calculate">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <Label fx:id="errorLabel" textFill="RED">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </Label>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="Family Before-Tax Income:" fx:id="beforeTaxLabel">
               <padding>
                  <Insets right="90.0" />
               </padding>
            </Label>
            <Label text="Total Annual Taxes Due:" fx:id="taxDueLabel">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <padding>
                  <Insets right="90.0" />
               </padding>
            </Label>
            <Label fx:id="totalLabel" text="Family Total Annual Income:">
               <HBox.margin>
                  <Insets right="90.0" />
               </HBox.margin>
            </Label>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="Total Annual Expenses:" fx:id="totalExpensesLabel">
               <padding>
                  <Insets right="90.0" />
               </padding>
            </Label>
            <Label text="Family Total Savings After Expenses:" fx:id="savingsLabel">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
               <padding>
                  <Insets right="90.0" />
               </padding>
            </Label>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
